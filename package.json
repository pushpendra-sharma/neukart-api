{
  "name": "neukart-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rimraf ./build && tsc",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=development node ./build/index.js",
